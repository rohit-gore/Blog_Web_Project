<%- include("partials/header") -%>

<body>
  <div class="compose">


    <h1>Compose</h1>

    <form class="" action="/compose" method="post">
      <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="postTitle" placeholder="Start Writing ..." autocomplete="off">

        <label>Post</label>
        <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
      </div>
      <button class="btn btn-primary" type="submit" name="compose">Publish</button>
    </form>
  </div>
</body>

<%- include("partials/footer") -%>
